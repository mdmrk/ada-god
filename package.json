    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.21.1",
    "npm-run-all2": "^6.1.2",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
